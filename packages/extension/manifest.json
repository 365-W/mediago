{
  "name": "chrome-plugin-demo",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/custom.js"
      ],
      "css": [
        "css/custom.css"
      ],
      "run_at": "document_start"
    }
  ]
}
