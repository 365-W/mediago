{
  "manifest_version": 2,
  "name": "Sample Web Extension",
  "version": "0.0.1",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
    "scripts": ["background.ts"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.tsx"]
    }
  ]
}
